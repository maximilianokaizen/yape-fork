// Para conectar con PostgreSQL
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_POSTGRES")
}

// Para conectar con MySQL
datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL_MYSQL")
}

// Para conectar con MongoDB
datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL_MONGODB")
}